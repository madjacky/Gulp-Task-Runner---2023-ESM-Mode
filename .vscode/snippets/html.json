{
  "svguse": {
    "scope": "html",
    "prefix": "g-use",
    "body": [
      "<svg class=\"\">",
      "  <use xlink:href=\"img/sprite.svg#$2\"></use>",
      "</svg>"
    ],
    "description": "Snippet for use svg sprites"
  },
  "theme-toggler": {
    "scope": "html",
    "prefix": "g-theme-toggle",
    "body": [
      "<button id=\"theme-toggle\" class=\"btn-reset icon-button theme-toggle\" title=\"Toggles light & dark\" aria-label=\"auto\" aria-live=\"polite\">",
      "  <svg class=\"sun-and-moon\" aria-hidden=\"true\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\">",
      "     <mask class=\"moon\" id=\"moon-mask\">",
      "       <rect x=\"0\" y=\"0\" width=\"100%\" height=\"100%\" fill=\"white\" />",
      "       <circle cx=\"24\" cy=\"10\" r=\"6\" fill=\"black\" />",
      "     </mask>",
      "     <circle class=\"sun\" cx=\"12\" cy=\"12\" r=\"6\" mask=\"url(#moon-mask)\" fill=\"currentColor\" />",
      "     <g class=\"sun-beams\" stroke=\"currentColor\">",
        "     <line x1=\"12\" y1=\"1\" x2=\"12\" y2=\"3\" />",
        "     <line x1=\"12\" y1=\"21\" x2=\"12\" y2=\"23\" />",
        "     <line x1=\"4.22\" y1=\"4.22\" x2=\"5.64\" y2=\"5.64\" />",
        "     <line x1=\"18.36\" y1=\"18.36\" x2=\"19.78\" y2=\"19.78\" />",
        "     <line x1=\"1\" y1=\"12\" x2=\"3\" y2=\"12\" />",
        "     <line x1=\"21\" y1=\"12\" x2=\"23\" y2=\"12\" />",
        "     <line x1=\"4.22\" y1=\"19.78\" x2=\"5.64\" y2=\"18.36\" />",
        "     <line x1=\"18.36\" y1=\"5.64\" x2=\"19.78\" y2=\"4.22\" />",
      "     </g>",
      "  </svg>",
      "</button>"
    ],
    "description": "Snippet for Dark & Light Theme Switcher"
  },
  "burger": {
    "scope": "html",
    "prefix": "g-burger",
    "body": [
      "<button class=\"btn-reset burger\" aria-label=\"Open Menu\" aria-controls=\"primary-navigation\" aria-expanded=\"false\"  data-burger>",
      "  <svg class=\"burger__icon\" fill=\"none\" width=\"32\" height=\"32\" vievbox=\"0 0 32 32\">",
      "     <rect class=\"burger__line burger__top\" width=\"24\" height=\"2\" x=\"4\" y=\"7.5\" rx=\"2\"></rect>",
      "     <rect class=\"burger__line burger__middle\" width=\"24\" height=\"2\" x=\"4\" y=\"15\" rx=\"2\"></rect>",
      "     <rect class=\"burger__line burger__bottom\" width=\"24\" height=\"2\" x=\"4\" y=\"22.5\" rx=\"2\"></rect>",
      "  </svg>",
      "</button>"
    ],
    "description": "Snippet for burger btn"
  },
  "checkbox": {
    "scope": "html",
    "prefix": "g-checkbox",
    "body": [
      "<label class=\"checkbox\">",
      "  <input type=\"checkbox\" name=\"\" class=\"input-reset checkbox__input\">",
      "  <span class=\"checkbox__caption\">$1</span>",
      "</label>"
    ],
    "description": "Snippet for custom checkbox"
  },
  "radio": {
    "scope": "html",
    "prefix": "g-radio",
    "body": [
      "<label class=\"radio\">",
      "  <input type=\"radio\" name=\"\" class=\"input-reset radio__input\">",
      "  <span class=\"radio__caption\">$1</span>",
      "</label>"
    ],
    "description": "Snippet for custom checkbox"
  },
  "radio tabs": {
    "scope": "html",
    "prefix": "g-radio-tabs",
    "body": [
      "<label class=\"radio-tab\">",
      "  <input type=\"radio\" name=\"\" value=\"\" class=\"input-reset radio-tab__input\">",
      "  <span class=\"radio-tab__caption\">$1</span>",
      "</label>"
    ],
    "description": "Snippet for custom checkbox"
  },
  "switch": {
    "scope": "html",
    "prefix": "g-switch",
    "body": [
      "<label class=\"switch\">",
      "  <input type=\"checkbox\" name=\"\" class=\"input-reset switch__input\">",
      "  <span class=\"switch__caption\">$1</span>",
      "</label>"
    ],
    "description": "Snippet for custom checkbox"
  },
  "navigation": {
    "scope": "html",
    "prefix": "g-nav",
    "body": [
      "<nav class=\"nav\" title=\"$1\">",
      "  <ul class=\"list-reset nav__list\">",
      "    <li class=\"nav__item\"><a href=\"#\" class=\"nav__link\"></a></li>",
      "    <li class=\"nav__item\"><a href=\"#\" class=\"nav__link\"></a></li>",
      "    <li class=\"nav__item\"><a href=\"#\" class=\"nav__link\"></a></li>",
      "  </ul>",
      "</nav>"
    ],
    "description": "Snippet for navigation"
  },
  "social": {
    "scope": "html",
    "prefix": "g-social",
    "body": [
      "<ul class=\"list-reset social\" title=\"$1\">",
      "  <li class=\"social__item\"><a href=\"#\" target=\"_blank\" class=\"social__link social__link--facebook\" aria-label=\"Our Facebook Page\"></a></li>",
      "  <li class=\"social__item\"><a href=\"#\" target=\"_blank\" class=\"social__link social__link--instagram\" aria-label=\"Our Instagram Profile\"></a></li>",
      "  <li class=\"social__item\"><a href=\"#\" target=\"_blank\" class=\"social__link social__link--twitter\" aria-label=\"Our Twitter Account\"></a></li>",
      "</ul>"
    ],
    "description": "Snippet for social links"
  },
  "basicform": {
    "prefix": "g-form",
    "body": [
      "<form action=\"#\" class=\"form\">",
      "  <label class=\"form__label\">",
      "    <input type=\"text\" name=\"Name\" class=\"input-reset form__input\" placeholder=\"Name\">",
      "  </label>",
      "  <button type=\"submit\" class=\"btn-reset form__btn\">Send</button>",
      "</form>"
    ],
    "description": "Snippet for basic form"
  },
  "basicswiper": {
    "prefix": "g-swiper",
    "body": [
      "<div class=\"swiper $1\">",
      "  <div class=\"swiper-wrapper\">",
      "     <div class=\"swiper-slide\">",
      "     </div>",
      "  </div>",
      "</div>"
    ],
    "description": "Snippet for basic swiper structure"
  },
  "graph modal Btn": {
    "prefix": "g-graph-btn",
    "body": [
      "<button class=\"btn-reset $1\" data-graph-path=\"$2\">$3</button>"
    ],
    "description": "Snippet for graph-modal btn"
  },
  "graph modal Structure": {
    "prefix": "g-graph-modal",
    "body": [
      "<div class=\"graph-modal\">",
      "  <div class=\"graph-modal__container\" role=\"dialog\" aria-modal=\"true\" data-graph-target=\"$1\">",
      "    <button class=\"btn-reset js-modal-close graph-modal__close\" aria-label=\"Close\"></button>",
      "    <div class=\"graph-modal__content\">$2</div>",
      "  </div>",
      "</div>"
    ],
    "description": "Snippet for graph-modal basic structure"
  },
  "picture": {
    "prefix": "g-picture",
    "body": [
      "<picture>",
      "  <source srcset=\"$1.avif\" type=\"image/avif\">",
      "  <source srcset=\"$1.webp\" type=\"image/webp\">",
      "  <img loading=\"lazy\" src=\"$1.$2\" class=\"image\" width=\"$4\" height=\"$5\" alt=\"$3\">",
      "</picture>"
    ],
    "description": "Snippet for basic logo structure"
  },
  "svgimg": {
    "scope": "html",
    "prefix": "g-svgimg",
    "body": [
      "<img loading=\"lazy\" src=\"$1.$2\" class=\"image\" width=\"$4\" height=\"$5\" alt=\"$3\">"
    ],
    "description": "Snippet for simple html svg images"
  },
  "tooltip": {
  "prefix": "g-tooltip",
    "body": [
      "<span class=\"tooltip\">",
      "  <button class=\"tooltip__btn\" aria-describedby=\"$1\">$2</button>",
      "  <span class=\"tooltip__txt\" role=\"tooltip\" id=\"$1\">",
      "    <span class=\"visually-hidden\">Tooltip: </span>",
      "    $3",
      "  </span>",
      "</span>"
    ],
    "description": "Snippet for tooltip"
  },
  "section": {
    "scope": "html",
    "prefix": "g-section",
    "body": [
      "<section class=\"section\">",
      "  <div class=\"container\">",
      "    <h2 class=\"section__title\"></h2>",
      "  </div>",
      "</section>"
    ],
    "description": "Snippet for Section"
  },
  "tabs html sctructure": {
    "prefix": "g-tabs",
    "body": [
      "<div class=\"tabs\" data-tabs=\"$1\">",
      "  <ul class=\"list-reset tabs__nav\">",
      "    <li class=\"tabs__nav-item\"><button class=\"btn-reset tabs__nav-btn\" type=\"button\">$2</button></li>",
      "    <li class=\"tabs__nav-item\"><button class=\"btn-reset tabs__nav-btn\" type=\"button\">$3</button></li>",
      "    <li class=\"tabs__nav-item\"><button class=\"btn-reset tabs__nav-btn\" type=\"button\">$4</button></li>",
      "  </ul>",
      "  <div class=\"tabs__content\">",
      "    <div class=\"tabs__panel\">$5</div>",
      "    <div class=\"tabs__panel\">$6</div>",
      "    <div class=\"tabs__panel\">$7</div>",
      "  </div>",
      "</div>"
    ],
    "description": "Snippet for basic tabs structure"
  }
}